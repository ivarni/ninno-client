    "eslint-plugin-react": "^6.10.3",
    "expect.js": "^0.3.1",
    "hjs-webpack": "^9.1.0",
    "husky": "^0.13.3",
    "mocha": "^3.3.0",
    "nyc": "^10.3.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0"
  },
