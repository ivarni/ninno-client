{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "import/no-extraneous-dependencies": [0],
        "import/prefer-default-export": [0],
        "indent": [2, 4, { "SwitchCase": 1 }],
        "react/jsx-indent": [2, 4],
        "react/jsx-filename-extension": [2, { "extensions": [".js"] }],
    }
}
